	String s1 = "army";
	String s2 = "mary";
	boolean flag = false;
	
		
	int l1 = 	s1.length();
	int l2 = 	s2.length();
	
	if(l1 == l2) {
		for(int i = 0; i < s1.length(); i++) {
			for(int j = 0; j < s2.length(); j++) {
			char ch = 	s1.charAt(i);
			char ch1 = 	s2.charAt(j);
			if(ch == ch1) {
				flag  = true;
				System.out.println("anagram");
			
				
			}else {
				flag = false;
				System.out.println("not anagram");
			}
			break;
			}
			
		}
			
		
		
	
	}else {
		System.out.println("both are not anagram");
	}
	
		
		
	}
